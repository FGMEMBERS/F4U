<?xml version="1.0"?>

<PropertyList>

  <!-- left-1 -->
  <submodel>
    <name>left-1</name>
    <model>Aircraft/F4U/Models/blaze.ac</model>
    <trigger>controls/armament/trigger</trigger>
    <speed>30</speed>
    <repeat>true</repeat>
    <delay>0.25</delay>
    <count>-1</count>
    <x-offset>-5.5</x-offset>
    <y-offset>-9.1</y-offset>
    <z-offset>-2.3</z-offset>
    <yaw-offset>0.0</yaw-offset>
    <pitch-offset alias="/orientation/pitch-deg"/>
    <life>0.005</life>
    <wind>true</wind>
    <weight>0.0</weight>
    <eda>0</eda>
    <cd>0</cd>
    <buoyancy>31</buoyancy>
  </submodel>

  <submodel>
    <name>left-1-tracer</name>
    <model>Aircraft/F4U/Models/tracer.xml</model>
    <trigger>controls/armament/trigger</trigger>
    <speed>2840.0</speed>
    <repeat>true</repeat>
    <delay>0.25</delay>
    <count>-1</count>
    <x-offset>-4.9</x-offset>
    <y-offset>-9.1</y-offset>
    <z-offset>-2.3</z-offset>
    <yaw-offset>0.0</yaw-offset>
    <pitch-offset>-1</pitch-offset>
    <life>2.0</life>
    <wind>false</wind>
    <weight>0.1069</weight>
    <cd>0.193</cd>
    <eda>0.00136354</eda> 
  </submodel>

	<submodel> 
      <name>left-1-smoke</name>
      <model>Aircraft/F4U/Models/smoke.xml</model>
      <trigger>controls/armament/trigger</trigger>
      <speed>10</speed>
      <repeat>true</repeat>
      <delay>0.25</delay>
      <count>-1</count>
    <x-offset>-40.0</x-offset>
    <y-offset>-9.1</y-offset>
    <z-offset>-2.3</z-offset>
      <yaw-offset>1.5</yaw-offset>
      <pitch-offset>1.8</pitch-offset>
	  <eda>10</eda>
      <wind>true</wind>
	  <buoyancy>33</buoyancy>
	  <life>1</life>
    </submodel>
