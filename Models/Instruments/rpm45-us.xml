<?xml version="1.0"?>
<PropertyList>

 <path>rpm45-us.ac</path>

 <animation>
  <type>material</type>
  <object-name>Face</object-name>
  <object-name>needle1</object-name>
  <emission>
    <factor-prop>/controls/lighting/instruments-norm</factor-prop>
    <red>0.5</red>
    <green>0.25</green>
    <blue>0.25</blue>
  </emission>
 </animation>

 <animation>
  <type>rotate</type>
  <object-name>needle1</object-name>
  <property>/engines/engine/rpm</property>
  <min>0</min>
  <max>4500</max>
  <interpolation>
   <entry>
    <ind>0</ind>
    <dep>-162</dep>
   </entry>
   <entry>
    <ind>4500</ind>
    <dep>162</dep>
   </entry>
  </interpolation>
  <axis>
   <x>-1</x>
   <y>0</y>
   <z>0</z>
  </axis>
 </animation>

</PropertyList>


